<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wikitude.example"
    android:versionCode="105"
    android:versionName="1.0.5" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_GPS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        
        <activity
            android:name="com.wikitude.example.SimpleARBrowserActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.wikitude.example.BusquedaSimpleActivity"
            android:label="@string/title_activity_busqueda_simple"
            android:parentActivityName="com.wikitude.example.SimpleARBrowserActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.wikitude.example.SimpleARBrowserActivity" />
        </activity>
        
        <activity
            android:name="com.wikitude.example.PDIDetalle"
            android:label="@string/title_activity_pdidetalle" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.ListaPDIsActivity"
            android:label="@string/title_activity_visualizar_lista" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.RegistrarUsuarioActivity"
            android:label="@string/title_activity_registrar_usuario" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.IniciarSesionActivity"
            android:label="@string/title_activity_iniciar_sesion" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.PerfilActivity"
            android:label="@string/title_activity_perfil" >
		</activity>
		
        <activity
            android:name="com.wikitude.example.BusquedaAvanzadaActivity"
            android:label="@string/title_activity_busqueda_avanzada" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.RespuestaBusquedaHandler"
            android:label="@string/title_activity_respuesta_busqueda_handler" >
        </activity>
        
        <activity
            android:name="com.wikitude.example.RegistroPDIActivity"
            android:label="@string/title_activity_registro_pdi" >
        </activity>
        <activity
            android:name="com.wikitude.example.ActualizarPDIActivity"
            android:label="@string/title_activity_actualizar_pdi" >
        </activity>
        <activity
            android:name="com.wikitude.example.ListaMisPDIActivity"
            android:label="@string/title_activity_lista_mis_pdi" >
        </activity>
        
    </application>

</manifest>
